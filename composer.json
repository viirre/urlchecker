{
    "name": "viirre/urlchecker",
    "description": "Checks how a URL is responding (eg. responding with status 200)",
    "keywords": [
        "viirre",
        "urlchecker",
        "check url status"
    ],
    "homepage": "https://github.com/viirre/urlchecker",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Schelin",
            "email": "victor@adaptivemedia.se",
            "homepage": "http://www.adaptivemedia.se",
            "role": "CTO & Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "guzzlehttp/guzzle": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Viirre\\UrlChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viirre\\UrlChecker\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}

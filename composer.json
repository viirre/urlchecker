{
    "name": "viirre/urlchecker",
    "description": "Simple utility package to check the status of a URL (status code, response time etc).",
    "keywords": [
        "viirre",
        "urlchecker",
        "check url status"
    ],
    "homepage": "https://github.com/viirre/urlchecker",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Eliasson",
            "email": "victor@adaptivemedia.se",
            "homepage": "https://www.adaptivemedia.se",
            "role": "CTO"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "6.*"
    },
    "autoload": {
        "psr-4": {
            "Viirre\\UrlChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viirre\\UrlChecker\\Test\\": "tests"
        }
    }
}
